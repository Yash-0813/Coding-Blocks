import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		String tmp="";
		for(int i=0;i<s.length();i++){
			if(s.charAt(i)>='A'&& s.charAt(i)<='Z'){
				tmp+=(s.charAt(i)+"").toLowerCase();
			}
			else{
				tmp+=(s.charAt(i)+"").toUpperCase();
			}
		}
		System.out.println(tmp);
    }
}
